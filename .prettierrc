{
  "semi": false,
  "endOfLine": "auto",
  "singleQuote": true,
  "trailingComma": "none",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "vueIndentScriptAndStyle": false,
  "jsxBracketSameLine:": true,
  "htmlWhitespaceSensitivity": "ignore",
  "wrapAttributes": true,
  "printWidth": 200,
  "overrides": [
    {
      "files": "*.html",
      "options": { "parser": "html" }
    }
  ]
}
