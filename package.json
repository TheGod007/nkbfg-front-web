{
  "name": "nkbfg-front-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue",
    "lint:fix": "eslint --fix --ext .js,.vue --ignore-path .eslintignore .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "stylelint:fix": "stylelint --fix src/**/*.{vue,scss,css,sass}",
    "stylelint": "stylelint src/**/*.{vue,scss,css,sass}",
    "cm": "git cz",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "npm run lint:fix",
    "src/**/*.{vue,scss,css,sass}": "npm run stylelint:fix"
  },
  "dependencies": {
    "@videojs-player/vue": "^1.0.0",
    "amfe-flexible": "^2.2.1",
    "axios": "^1.6.7",
    "echarts": "^5.5.0",
    "element-plus": "^2.5.6",
    "path": "^0.12.7",
    "pinia": "^2.1.7",
    "video.js": "^7.21.5",
    "vue": "^3.4.15",
    "vue-router": "4"
  },
  "devDependencies": {
    "@iconify-json/ep": "^1.1.14",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-vue": "^5.0.3",
    "commitizen": "^4.3.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.21.1",
    "git-cz": "^4.9.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "sass": "^1.71.0",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-icons": "^0.18.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-top-level-await": "^1.4.1",
    "vue-tsc": "^1.8.27"
  }
}
